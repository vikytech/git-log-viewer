<!DOCTYPE html>
<html>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  <body>
      <table>
          <th>Commits ({{commitsWithStatus.unreadCount}})</th>
          <tbody>
            {{#commitsWithStatus}}
                {{#commits}}
                 <tr>
                    <td>
                        <a class="{{status}}" href="/home/commit?hash={{sha}}"><input type="checkbox"/> {{message}}</a>
                    </td>
                 </tr>
                {{/commits}}
            {{/commitsWithStatus}}
            </tbody>
      </table>
  </body>
</html>